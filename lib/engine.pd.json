{
	"pd": {
		"options": {
			"pd": "/Applications/Pd-0.45-5-64bit.app/Contents/Resources/bin/pd"
			, "encoding": "ascii"
			, "read": 8005
			, "write": 8006
			, "basepath": ""
		}
		, "flags": {
			"r": 44100
			, "audiobuf": 6
			, "nogui": false
			, "nodac": false
			, "noadc": false
			, "nomidi": true
			, "noautopatch": false
			, "noloadbang": false
			, "noprefs": true
			, "stderr": true
			, "listdev": false
			, "verbose": false
			, "batch": false
			, "send": "netsend send ready;"
			, "path": ["pd", "pd/blib"]
			, "open": "./manager.pd"
		}
		, "process": false
		, "dsp": false
	}
	, "schema": {
		"pd": {
			"type": "object"
			, "properties": {
				"options": {
					"title": "Options"
					, "type": "object"
					, "columns": true
					, "fieldset": true
					, "disable": ["engine", "pd", "process"]
					, "properties": {
						"pd": {
							"title": "Pd"
							, "type": "string"
						}
						, "encoding": {
							"type": "enum"
							, "values": ["ascii", "utf-8", "base64", "hex"]
						}
					}
				}
				, "flags": {
					"title": "Flags"
					, "type": "object"
					, "columns": true
					, "fieldset": true
					, "disable": ["engine", "pd", "process"]
					, "properties": {
						"r": {
							"title": "sample rate"
							, "type": "enum"
							, "values": [44100, 22050]
						}
						, "audiobuf": {
							"title": "audio buffer"
							, "description": "size of audio I/O buffer in msec"
						}
						, "nogui": {
							"description": "suppress starting the GUI"
						}
						, "nodac": {
							"description": "suppress audio output"
						}
						, "noadc": {
							"description": "suppress audio input"
						}
						, "nomidi": {
							"description": "suppress MIDI input and output"
						}
						, "noautopatch": {
							"description": "suppress auto-connecting to new objects"
						}
						, "noloadbang": {
							"description": "suppress all loadbangs"
						}
						, "listdev": {
							"description": "list audio and MIDI devices"
						}
						, "verbose": {
							"description": "extra printout on startup when searching for files"
						}
						, "nogui": {
							"description": "suppress starting the GUI"
						}
						, "batch": {
							"description": "You should probably also specify -nosound. With these flag Pd runs as quickly as it can without waiting for real time"
						}
						, "send": {
							"description": "send a message at startup (after patches are loaded)"
						}
						, "path": {
							"type": "array"
							, "title": "Path"
							, "items": {
								"type": "string"
								, "default": ""
							}
						}
						, "open": {
							"description": "open file(s) on startup"
						}
					}
				}
				, "process": {
					"title": "Spawn"
					, "important": true
				}
				, "dsp": {
					"title": "Compute Audio"
					, "disabled": true
					, "enable": ["engine", "pd", "process"]
				}
			}
		}
	}
}
